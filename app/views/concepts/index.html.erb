<p id="notice"><%= notice %></p>

<section id="services">
  <div class="container">
    <div class="row">

    
<h1>Listing Concepts</h1>

<table>
  <thead>
    <tr>
      <th>Infraccion</th>
      <th>Monto</th>
      <th>Corralon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @concepts.each do |concept| %>
      <tr>
        <td><%= concept.infraccion %></td>
        <td><%= concept.monto %></td>
        <td><%= concept.corralon %></td>
        <td><%= link_to 'Show', concept %></td>
        <td><%= link_to 'Edit', edit_concept_path(concept) %></td>
        <td><%= link_to 'Destroy', concept, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
  </div>
</section>

<%= link_to 'New Concept', new_concept_path %>
